# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_from_botfather

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@host:port/database

# Redis (Railway Redis - optional)
REDIS_URL=redis://default:password@host:port

# Google Sheets (optional)
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id

# App
PORT=3001
NODE_ENV=development
FRONTEND_URL=http://localhost:5173
WEBAPP_URL=https://your-app.up.railway.app

# Course settings
COURSE_START_DATE=2025-01-01
TRAINER_TELEGRAM_ID=123456789

# Admin notifications
ADMIN_CHAT_ID=-100123456789

# FatSecret API (for product search)
FATSECRET_CLIENT_ID=your_fatsecret_client_id
FATSECRET_CLIENT_SECRET=your_fatsecret_client_secret
FATSECRET_ENABLED=true
# Optional: HTTP proxy for FatSecret (to bypass IP restrictions)
# Format: http://user:password@proxy-host:port or http://proxy-host:port
# Services: Fixie, QuotaGuard, IPBurger, or any HTTPS proxy
FATSECRET_PROXY_URL=
